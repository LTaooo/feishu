{
    "name": "ltaooo/feishu",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ltaooo\\FeiShu\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.60",
        "pestphp/pest": "^2.34"
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "./vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
